/*
n=3
 *
 **
 ***

 */
import java.util.Scanner;
public class Pattern11 {


	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int row=1,col;
		int cnt;
		while(row<=n)
		{
			col=1;
			cnt=n-row+1;
			while(col<=row)
			{
				System.out.print(cnt);
				cnt++;
				col++;
			}
			System.out.println();
			row++;
		}


	}

}
