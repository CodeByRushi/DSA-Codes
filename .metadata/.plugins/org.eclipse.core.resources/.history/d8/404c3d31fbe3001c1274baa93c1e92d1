import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n=1;int i=1;
		while(true)
        {
            if(n>x)
            {
                break;
            }
            int ans = (3*(i)+2);
            if(ans % 4 != 0)
            {
                System.out.print(ans + " ");
                n++;

            }
            i++;
        }

	
	}
}
