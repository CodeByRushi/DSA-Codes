import java.lang.Math;
import java.util.Scanner;
public class Salary_Evaluation {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int basic = sc.nextInt();
		char ch = sc.next().charAt(0);
		double hra = ((2/10)* basic);System.out.println(hra);
		double da = (5/10) * basic;System.out.println(da);
		double pf = (11/100) * basic;System.out.println(pf);
		double allow;
		if(ch=='A')
		{
			allow=1700;
		}
		else if(ch == 'B')
		{
			allow=1500;
		}
		else
		{
			allow=1300;
		}
		double total = hra + da + pf + allow;
		System.out.println(total);
		total = Math.round(total);
		long l = (long)total;
		//System.out.print(l);
	}
}