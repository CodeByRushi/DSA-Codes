
public class DataTypes {

	public static void main(String[] args) {
		
		//For Integral Number
		byte b=9;//size is 1 byte
		short s= 10;//size is 2 byte
		int a = 20;//size is 4 byte
		long l = 30; // size is 8 byte
		
		
		
		//For Decimal Number
		float f = 10.25f; // size is 4 bytes
		double d = 20.5;// size is 8 byte
		
		//For Character
		char ch='a';
		
		//For boolean
		boolean p1=true;
		boolean p2=false;
		


	}

}
