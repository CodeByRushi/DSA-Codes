/*
n=4
1
22
333
4444
  
 */
import java.util.Scanner;
public class Pattern8_ThreeTogether {


	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int row=1,col;
        while(row<=n)
        {
        	//first triangle
           	col=1;
            while(col<=(n-row+1))
            {
                System.out.print(col);
                col++;
            }
            System.out.println();
            row++;
        }
		
	}

}
