
public class Bitwise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 2;
		System.out.println("a&b = "+(int)(a&b));
		System.out.println("a|b = "+(int)(a|b));
		System.out.println("a^b = "+(int)(a^b));
		System.out.println("~a = "+(int)(~a));
		System.out.println("a>>1"+(int)(a>>1));
		System.out.println("a<<1"+(int)(a<<1));
	}

}
