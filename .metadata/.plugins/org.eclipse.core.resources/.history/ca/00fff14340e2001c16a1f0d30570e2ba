/*
n=3
1
23
456
 */
import java.util.Scanner;

public class Pattern10_Traingular {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int row=1;
		while(row<=n)
		{
			
			while(n>=1)
			{
				System.out.print('*');
				n--;
			}
			System.out.println();
			row++;
		}

	}

}
