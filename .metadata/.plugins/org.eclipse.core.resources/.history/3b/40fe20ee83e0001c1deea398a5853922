
public class DataTypes {

	public static void main(String[] args) {
		
		//For Integral Number
		byte b=9;//size is 1 byte
		short s= 10;//size is 2 byte
		int a = 20;//size is 4 byte
		long l = 30; // size is 8 byte
		
		
		
		//For Decimal Number
		float f = 10.99999999f; // size is 4 bytes
		System.out.println(f);
		double d = 10.99999999;// size is 8 byte
		System.out.println(d);
		//For Character
		char ch='a';
		
		//For boolean
		boolean p1=true;
		boolean p2=false;
		
		int var$=3;
		System.out.println(var$);
		


	}

}
