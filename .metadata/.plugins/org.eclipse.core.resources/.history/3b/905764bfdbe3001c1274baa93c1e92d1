import java.util.Scanner;

public class Sample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 Scanner sc = new Scanner(System.in);
			int n = sc.nextInt();
	        boolean b=false;
	        for(int i=2 ; i <=n ; i++)
	        {
	            for(int j=2;j<i;j++)
	            {
	                if(i % j == 0)
	                {
	                    b=true;
	                    break;
	                }
	            }
	            if(b==false)
	            {
	                System.out.println(i);
	            }
	            
	        }
	}

}
